{
    "name": "hipster/user-discounts",
    "description": "A Laravel package for user-level discounts with deterministic stacking and comprehensive auditing",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "discounts", "users", "ecommerce"],
    "authors": [
        {
            "name": "Hipster Team",
            "email": "team@hipster.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Hipster\\UserDiscounts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hipster\\UserDiscounts\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hipster\\UserDiscounts\\UserDiscountsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

